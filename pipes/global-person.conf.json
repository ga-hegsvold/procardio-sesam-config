{
  "_id": "global-person",
  "type": "pipe",
  "source": {
    "type": "merge",
    "datasets": ["fhir-person-enrich f-person", "fhir-patient-enrich f-patient"],
    "equality_sets": [
      ["f-person.id", "f-patient.id"]
    ],
    "identity": "first",
    "if_source_empty": "accept",
    "strategy": "default",
    "version": 2
  },
  "sink": {
    "type": "dataset",
    "indexes": ["$ids"]
  },
  "metadata": {
    "global": true,
    "tags": ["fhir"]
  }
}
