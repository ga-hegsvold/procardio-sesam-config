{
  "_id": "fhir-diagnosticreport-enrich",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "fhir-diagnosticreport-collect"
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"],
        ["add", "rdf:type",
          ["ni", "fhir", "_S.resourceType"]
        ],
        ["add", "media-ni",
          ["values",
            ["apply", "media-links", "_S.media"]
          ]
        ],
        ["add", "performer-ni",
          ["ni",
            ["concat", "fhir-",
              ["lower",
                ["first",
                  ["split", "/", "_S.performer.reference"]
                ]
              ]
            ],
            ["last",
              ["split", "/", "_S.performer.reference"]
            ]
          ]
        ],
        ["add", "subject-ni",
          ["ni",
            ["concat", "fhir-",
              ["lower",
                ["first",
                  ["split", "/", "_S.subject.reference"]
                ]
              ]
            ],
            ["last",
              ["split", "/", "_S.subject.reference"]
            ]
          ]
        ]
      ],
      "media-links": [
        ["add", "tmp-ni",
          ["ni",
            ["concat", "fhir-",
              ["lower",
                ["first",
                  ["split", "/", "_S.link.reference"]
                ]
              ]
            ],
            ["last",
              ["split", "/", "_S.link.reference"]
            ]
          ]
        ]
      ]
    }
  },
  "metadata": {
    "tags": ["fhir", "enrich"]
  },
  "add_namespaces": true,
  "namespaces": {
    "identity": "fhir-diagnosticreport",
    "property": "fhir-diagnosticreport"
  }
}
