{
  "_id": "fhir-person-enrich",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "fhir-person-collect"
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"],
        ["add", "rdf:type",
          ["ni", "fhir", "_S.resourceType"]
        ]
      ]
    }
  },
  "add_namespaces": true,
  "namespaces": {
    "identity": "fhir-person",
    "property": "fhir-person"
  }
}
